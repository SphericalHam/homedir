### Restrictive defaults.
Host *
CheckHostIP yes
Compression yes
CompressionLevel 9
ForwardAgent no
ForwardX11 no
GatewayPorts no
KeepAlive yes
PasswordAuthentication no
Protocol 2
SkeyAuthentication no
GSSAPIAuthentication no
StrictHostKeyChecking yes

# Only use these settings on a TRUSTED machine!
#ControlMaster auto
#ControlPath ~/.ssh/cm_socket/%l-%r@%h:%p
